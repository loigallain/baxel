{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "noResolve": false,
    "noImplicitAny": false,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "experimentalDecorators": true,
    "isolatedModules": false,
    "useDefineForClassFields": true,
    "lib": [
      "dom",
      "es2015.promise",
      "es5",
      "es6"
    ],
    "baseUrl":"src",
    "paths": {
      "@interfaces/*": ["interfaces/*"],
      "@utils/*": ["utils/*"],
      "@engine/*": ["engine/*"],
      "@world/*": ["world/*"],
      "@player/*": ["player/*"],
      "@ui/*": ["ui/*"],
      

    },
    "types": [
      "babylonjs",
      "babylonjs-gui",
      "babylonjs-materials"
    ],
    "declaration": true,
    "outDir": "./build/types"
  },
  "files": [
    "./src/index.ts"
  ]
}